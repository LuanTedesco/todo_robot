<div id="<%= dom_id task %>">
  <p>
    <strong>Tittle:</strong>
    <%= task.tittle %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= task.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= task.end_date %>
  </p>

  <p>
    <strong>Result:</strong>
    <%= task.result %>
  </p>

  <p>
    <strong>Finished:</strong>
    <%= task.finished %>
  </p>

</div>
